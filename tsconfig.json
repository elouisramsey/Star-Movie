{
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "strictNullChecks": true,
    "paths": {
      "components/*": ["src/components/*"],
      "assets/*": ["assets/*"],
      "screens/*": ["src/screens/*"],
      "types/*": ["src/types/*"],
      "config/*": ["src/config/*"],
      "navigation/*": ["src/navigation/*"],
      "context/*": ["src/context/*"]
    }
  },
  "extends": "expo/tsconfig.base",
  "typeRoots": ["./types"]
}
